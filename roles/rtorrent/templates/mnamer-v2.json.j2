{
    "api_key_omdb": null,
    "api_key_tmdb": null,
    "api_key_tvdb": null,
    "api_key_tvmaze": null,
    "batch": false,
    "episode_api": "tvdb",
    "episode_directory": "/mnt/data/anime/{series}/Season {season:02}",
    "episode_format": "{series} - S{season:02}E{episode:02} - {title}.{extension}",
    "hits": 5,
    "ignore": [
        ".*sample.*",
        "^RARBG.*"
    ],
    "language": "en",
    "lower": false,
    "mask": [
        ".avi",
        ".m4v",
        ".mp4",
        ".mkv",
        ".ts",
        ".wmv",
        ".srt",
        ".idx",
        ".sub"
    ],
    "movie_api": "tmdb",
    "movie_directory": "/mnt/data/movies/{name}",
    "movie_format": "{name}.{extension}",
    "no_guess": true,
    "no_overwrite": true,
    "no_style": false,
    "recurse": false,
    "replace_after": {
        "&": "and",
        ";": ",",
        "@": "at",
	":": " ",
	"'": ""
    },
    "replace_before": {},
    "scene": false,
    "verbose": false
}
